//program to print 5 times "Marvellous" on screen.
#include<iostream>
using namespace std;
class Rohan
{
  public:
     void display()
     {
         int i=0;
         for(i=1;i<=5;i++)
         {
             cout<<"Marvellous"<<"\n";

         }
     }
};
int main()
{
    Rohan obj;
    obj.display();
    return 0;
    
}
