//program to print 5 to 1 numbers on screen
#include<iostream>
using namespace std;
class Number
{
    public:
    void display()
    {
        int i=5;
        for(i=5;i>0;i--)
        {
            cout<<i<<"\n";
            
        }
            
        

    }
};
int main()
{
    Number obj;
    obj.display();
    return 0;

}
